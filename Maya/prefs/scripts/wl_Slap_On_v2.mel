shadingNode -n l_arm_worldScale_div -asUtility multiplyDivide;
shadingNode -n l_arm_IK_joint_distance_div -asUtility multiplyDivide;

//CREATE BLENDNODES
shadingNode -n l_upArm_IK_blend -asUtility blendColors;
shadingNode -n l_loArm_IK_blend -asUtility blendColors;

//CONNECT IT ALL
connectAttr -f l_elbow_IK_anim.stretch l_upArm_IK_blend.blender;
connectAttr -f l_elbow_IK_anim.stretch l_loArm_IK_blend.blender;

