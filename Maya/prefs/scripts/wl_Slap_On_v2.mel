select -add l_arm_ikHandle ;
poleVectorConstraint -weight 1;

parent l_arm_ikHandle l_IK_loArm_end_loc ;


//CREATE MULTIDIV NODES
shadingNode -n l_arm_IK_joint_toEndDist_div -asUtility multiplyDivide;
shadingNode -n l_loArm_end_dist_mult -asUtility multiplyDivide;
shadingNode -n l_arm_IK_joint_toKneeDist_div -asUtility multiplyDivide;
