connectAttr -f l_arm_worldScale_div.outputX l_loArm_dist_mult.input2X;
connectAttr -f l_arm_worldScale_div.outputX l_arm_IK_joint_distance_div.input2X;


$lengthAttr = `getAttr l_loArm_IK_joint.tx`;
