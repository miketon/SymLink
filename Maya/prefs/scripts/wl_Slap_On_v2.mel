//CREATE THE IKRPSOLVER
select -r l_upArm_IK_joint;
select -add l_hand_IK_joint;
ikHandle -n l_arm_ikHandle -startJoint l_upArm_IK_joint -endEffector l_hand_IK_joint -sol ikRPsolver;

//CREATE THE MEASURE NODES

