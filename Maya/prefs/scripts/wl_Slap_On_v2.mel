setAttr -e-keyable true l_upLeg_FK_anim.gimbal_animVis;

addAttr -ln "gimbal_animVis"  -at double  -min 0 -max 1 l_loLeg_FK_anim;
setAttr -e-keyable true l_loLeg_FK_anim.gimbal_animVis;

addAttr -ln "gimbal_animVis"  -at double  -min 0 -max 1 l_foot_FK_anim;
setAttr -e-keyable true l_foot_FK_anim.gimbal_animVis;

addAttr -ln "gimbal_animVis"  -at double  -min 0 -max 1 l_toe_FK_anim;
setAttr -e-keyable true l_toe_FK_anim.gimbal_animVis;

//
//CONNECT THE VIS OF GIMBAL ANIMS TO FK ANIMS
//

connectAttr -f l_upLeg_FK_anim.gimbal_animVis l_upLeg_gimbal_animShape.visibility;

connectAttr -f l_loLeg_FK_anim.gimbal_animVis l_loLeg_gimbal_animShape.visibility;

connectAttr -f l_foot_FK_anim.gimbal_animVis l_foot_gimbal_animShape.visibility;

connectAttr -f l_toe_FK_anim.gimbal_animVis l_toe_gimbal_animShape.visibility;



//Parent constrain the FK joints to the FK anims

select -r l_upLeg_gimbal_anim ;
select -add l_upLeg_FK_joint ;
doCreateParentConstraintArgList 1 { "1","0","0","0","0","0","0","1","","1" };

select -r l_loLeg_gimbal_anim ;
select -add l_loLeg_FK_joint ;
doCreateParentConstraintArgList 1 { "1","0","0","0","0","0","0","1","","1" };

