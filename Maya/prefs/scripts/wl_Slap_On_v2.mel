select -r torso_01_result_joint;
select -add torso_end_result_joint;
ikHandle -sol ikSplineSolver -ns 6 -n torso_ikHandle;

rename "curve1" "torso_crv";


select -r chest_joint hip_joint torso_crv;

newSkinCluster "-toSelectedBones -normalizeWeights 1 -maximumInfluences 2 -obeyMaxInfluences true -dropoffRate 4 -removeUnusedInfluence false,multipleBindPose,1";

$skinCluster = `findRelatedSkinCluster torso_crv`;
