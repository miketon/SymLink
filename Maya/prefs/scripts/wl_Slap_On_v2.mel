connectAttr -f r_leg_worldScale_div.outputX r_loLeg_dist_mult.input2X;
connectAttr -f r_leg_worldScale_div.outputX r_leg_IK_joint_distance_div.input2X;


$lengthAttr = `getAttr r_loLeg_IK_joint.tx`;
