connectAttr -f l_IK_elbow_locShape.worldPosition[0] l_arm_IK_joint_toEndDistShape.startPoint;
connectAttr -f l_IK_loArm_end_locShape.worldPosition[0] l_arm_IK_joint_toEndDistShape.endPoint;

//SNAP THE LOCATORS TO THE CORRECT POSITIONS

select -r l_upArm_IK_joint ;
select -add l_IK_upArm_loc ;
delete `pointConstraint -offset 0 0 0 -weight 1`;
delete `orientConstraint -offset 0 0 0 -weight 1`;

select -r l_hand_IK_joint ;
