connectAttr -f l_arm_IK_joint_toEndDist_div.outputX l_loArm_IK_blend.color1R;

setAttr "l_arm_IK_joint_toKneeDist_div.operation" 2;
connectAttr -f l_arm_IK_joint_toElbowDistShape.distance l_arm_IK_joint_toKneeDist_div.input1X;
connectAttr -f l_loArm_dist_mult.outputX l_arm_IK_joint_toKneeDist_div.input2X;

connectAttr -f l_arm_IK_joint_toKneeDist_div.outputX l_upArm_IK_blend.color1R; 


setAttr "l_IK_loArm_end_loc.translateX" 0;
setDrivenKeyframe -currentDriver l_arm_IK_joint_distance_div.outputX l_upArm_IK_blend.color2R;
setDrivenKeyframe -currentDriver l_arm_IK_joint_distance_div.outputX l_loArm_IK_blend.color2R;
setAttr "l_IK_loArm_end_loc.translateX" 10;
setDrivenKeyframe -currentDriver l_arm_IK_joint_distance_div.outputX l_upArm_IK_blend.color2R;
setDrivenKeyframe -currentDriver l_arm_IK_joint_distance_div.outputX l_loArm_IK_blend.color2R;
setAttr "l_IK_loArm_end_loc.translateX" 0;

$elbowLength = `getAttr l_loArm_IK_joint.tx`;
$footLength = `getAttr l_hand_IK_joint.tx`;

keyframe -option over -index 0 -absolute -floatChange ($elbowLength + $footLength) l_loArm_IK_blend_color2R ;
keyframe -index 0 -absolute -valueChange 1 l_loArm_IK_blend_color2R ;

keyframe -option over -index 1 -absolute -floatChange (($elbowLength + $footLength)*2) l_loArm_IK_blend_color2R ;
keyframe -index 1 -absolute -valueChange 2 l_loArm_IK_blend_color2R ;

keyframe -option over -index 0 -absolute -floatChange ($elbowLength + $footLength) l_upArm_IK_blend_color2R ;
keyframe -index 0 -absolute -valueChange 1 l_upArm_IK_blend_color2R ;

