js_copyPivot;

select -r l_foot_FK_joint ;
select -add l_foot_gimbal_anim ;
source js_copyPivot;
js_copyPivot;

select -r l_foot_gimbal_anim ;
select -add l_foot_FK_joint ;
doCreateParentConstraintArgList 1 { "1","0","0","0","0","0","0","1","","1" };

select -r l_toe_gimbal_anim ;
