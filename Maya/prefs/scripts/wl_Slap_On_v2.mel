delete l_foot_IK_joint1 ;

parent l_loLeg_end_joint l_loLeg_IK_joint ;

copyAttr -v l_foot_IK_joint.tx l_loLeg_end_joint.tx;

parent l_foot_IK_joint l_leg_joint_trans ;


//ORIENT IK JOINT CHAINS SO THAT IT DOESN'T POP FORWARD WHEN YOU CREATE THE IK STRETCH

select -r l_upLeg_IK_joint ;
joint -e  -oj xzy -secondaryAxisOrient zup -ch -zso;



//STRETCHY IK ON L_LEG
/*
KEY:
leg
upLeg
loLeg
knee
*/

//CREATE THE IKRPSOLVER
select -r l_upLeg_IK_joint;
select -add l_loLeg_end_joint;
ikHandle -n l_leg_ikHandle -startJoint l_upLeg_IK_joint -endEffector l_loLeg_end_joint -sol ikRPsolver;

//CREATE THE MEASURE NODES

createNode -n l_leg_IK_joint_distanceShape distanceDimShape;
createNode -n l_IK_loLeg_end_locShape locator;
createNode -n l_IK_upLeg_locShape locator;
connectAttr -f l_IK_upLeg_locShape.worldPosition[0] l_leg_IK_joint_distanceShape.startPoint;
connectAttr -f l_IK_loLeg_end_locShape.worldPosition[0] l_leg_IK_joint_distanceShape.endPoint;

createNode -n l_leg_IK_joint_toElbowDistShape distanceDimShape;
createNode -n l_IK_knee_locShape locator;
connectAttr -f l_IK_upLeg_locShape.worldPosition[0] l_leg_IK_joint_toElbowDistShape.startPoint;
connectAttr -f l_IK_knee_locShape.worldPosition[0] l_leg_IK_joint_toElbowDistShape.endPoint;

createNode -n l_leg_IK_joint_toEndDistShape distanceDimShape;
connectAttr -f l_IK_knee_locShape.worldPosition[0] l_leg_IK_joint_toEndDistShape.startPoint;
connectAttr -f l_IK_loLeg_end_locShape.worldPosition[0] l_leg_IK_joint_toEndDistShape.endPoint;

//SNAP THE LOCATORS TO THE CORRECT POSITIONS

select -r l_upLeg_IK_joint ;
select -add l_IK_upLeg_loc ;
delete `pointConstraint -offset 0 0 0 -weight 1`;
delete `orientConstraint -offset 0 0 0 -weight 1`;

select -r l_loLeg_end_joint ;
select -add l_IK_loLeg_end_loc ;
delete `pointConstraint -offset 0 0 0 -weight 1`;
delete `orientConstraint -offset 0 0 0 -weight 1`;

select -r l_knee_IK_anim ;
select -add l_IK_knee_loc ;
delete `pointConstraint -offset 0 0 0 -weight 1`;
delete `orientConstraint -offset 0 0 0 -weight 1`;

select -r l_IK_loLeg_end_loc ;
select -add l_IK_upLeg_loc ;
select -add l_IK_knee_loc ;
makeIdentity -apply true -t 1 -r 1 -s 1 -n 0  ;

//IKHANDLE
select -r l_IK_knee_loc ;
select -add l_leg_ikHandle ;
poleVectorConstraint -weight 1;

parent l_leg_ikHandle l_IK_loLeg_end_loc ;


//CREATE MULTIDIV NODES
shadingNode -n l_leg_IK_joint_toEndDist_div -asUtility multiplyDivide;
shadingNode -n l_loLeg_end_dist_mult -asUtility multiplyDivide;
shadingNode -n l_leg_IK_joint_toKneeDist_div -asUtility multiplyDivide;
shadingNode -n l_loLeg_dist_mult -asUtility multiplyDivide;
shadingNode -n l_leg_worldScale_div -asUtility multiplyDivide;
shadingNode -n l_leg_IK_joint_distance_div -asUtility multiplyDivide;

//CREATE BLENDNODES
shadingNode -n l_upLeg_IK_blend -asUtility blendColors;
shadingNode -n l_loLeg_IK_blend -asUtility blendColors;

//CONNECT IT ALL
connectAttr -f l_knee_IK_anim.stretch l_upLeg_IK_blend.blender;
connectAttr -f l_knee_IK_anim.stretch l_loLeg_IK_blend.blender;

setAttr "l_leg_worldScale_div.operation" 2;
setAttr "l_leg_IK_joint_distance_div.operation" 2;

connectAttr -f all_anim.worldScale l_leg_worldScale_div.input1X ;

connectAttr -f l_leg_worldScale_div.outputX l_loLeg_end_dist_mult.input2X;
connectAttr -f l_leg_worldScale_div.outputX l_loLeg_dist_mult.input2X;
connectAttr -f l_leg_worldScale_div.outputX l_leg_IK_joint_distance_div.input2X;


$lengthAttr = `getAttr l_loLeg_IK_joint.tx`;
setAttr l_loLeg_dist_mult.input1X $lengthAttr;
$lengthAttr = `getAttr l_loLeg_end_joint.tx`;
setAttr l_loLeg_end_dist_mult.input1X $lengthAttr;

connectAttr -f l_leg_IK_joint_distanceShape.distance l_leg_IK_joint_distance_div.input1X;

setAttr "l_leg_IK_joint_toEndDist_div.operation" 2;
connectAttr -f l_leg_IK_joint_toEndDistShape.distance l_leg_IK_joint_toEndDist_div.input1X;
connectAttr -f l_loLeg_end_dist_mult.outputX l_leg_IK_joint_toEndDist_div.input2X; 

connectAttr -f l_leg_IK_joint_toEndDist_div.outputX l_loLeg_IK_blend.color1R;

setAttr "l_leg_IK_joint_toKneeDist_div.operation" 2;
connectAttr -f l_leg_IK_joint_toElbowDistShape.distance l_leg_IK_joint_toKneeDist_div.input1X;
connectAttr -f l_loLeg_dist_mult.outputX l_leg_IK_joint_toKneeDist_div.input2X;

connectAttr -f l_leg_IK_joint_toKneeDist_div.outputX l_upLeg_IK_blend.color1R; 


setAttr "l_IK_loLeg_end_loc.translateY" 0;
setDrivenKeyframe -currentDriver l_leg_IK_joint_distance_div.outputX l_upLeg_IK_blend.color2R;
setDrivenKeyframe -currentDriver l_leg_IK_joint_distance_div.outputX l_loLeg_IK_blend.color2R;
setAttr "l_IK_loLeg_end_loc.translateY" -10;
setDrivenKeyframe -currentDriver l_leg_IK_joint_distance_div.outputX l_upLeg_IK_blend.color2R;
setDrivenKeyframe -currentDriver l_leg_IK_joint_distance_div.outputX l_loLeg_IK_blend.color2R;
setAttr "l_IK_loLeg_end_loc.translateY" 0;

$kneeLength = `getAttr l_loLeg_IK_joint.tx`;
$footLength = `getAttr l_loLeg_end_joint.tx`;

keyframe -option over -index 0 -absolute -floatChange ($kneeLength + $footLength) l_loLeg_IK_blend_color2R ;
