setAttr -lock true -keyable false -channelBox false "all_anim.sx";
setAttr -lock true -keyable false -channelBox false "all_anim.sz";



//
//ADD THE STRETCHY SPLINE
//

shadingNode -n spine_info -asUtility curveInfo;

connectAttr torso_crvShape.worldSpace spine_info.inputCurve;

shadingNode -n torso_stretch_div -asUtility multiplyDivide;
setAttr "torso_stretch_div.operation" 2;

connectAttr -f spine_info.arcLength torso_stretch_div.input1X;

shadingNode -n torso_worldScale_mult -asUtility multiplyDivide;

connectAttr -f all_anim.worldScale torso_worldScale_mult.input1X;

connectAttr -f torso_worldScale_mult.outputX torso_stretch_div.input2X;

setAttr "torso_worldScale_mult.input2X" `getAttr spine_info.arcLength`;


//CONNECT THE TRANSLATE X OF THE JOINTS TO THE STRETCH

shadingNode -n torso_stretch_mult -asUtility multiplyDivide;

connectAttr -f torso_stretch_div.outputX torso_stretch_mult.input1X;
setAttr "torso_stretch_mult.input2X" `getAttr torso_02_result_joint.tx`;

connectAttr -f torso_stretch_mult.outputX torso_end_result_joint.translateX;
connectAttr -f torso_stretch_mult.outputX torso_06_result_joint.translateX;
connectAttr -f torso_stretch_mult.outputX torso_05_result_joint.translateX;
connectAttr -f torso_stretch_mult.outputX torso_04_result_joint.translateX;
connectAttr -f torso_stretch_mult.outputX torso_03_result_joint.translateX;
connectAttr -f torso_stretch_mult.outputX torso_02_result_joint.translateX;


