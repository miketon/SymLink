parent l_toe_gimbal_anim l_toe_FK_anim;
parent l_toe_FK_anim l_foot_gimbal_anim;
parent l_foot_gimbal_anim l_foot_FK_anim;
parent l_foot_FK_anim l_loLeg_gimbal_anim;
parent l_loLeg_gimbal_anim l_loLeg_FK_anim;
parent l_loLeg_FK_anim l_upLeg_gimbal_anim;
parent l_upLeg_gimbal_anim l_upLeg_FK_anim;

group -n l_leg_FK_anim_grp l_leg_FK_anim_trans;

parent -w l_leg_FK_anim_grp;

move 0 0 0 l_leg_FK_anim_grp.scalePivot l_leg_FK_anim_grp.rotatePivot ;

setAttr -lock true "l_leg_FK_anim_trans.rx";
setAttr -lock true "l_leg_FK_anim_trans.ry";
